{
  "name": "Volleyball World Hide Replay Length",
  "description": "Hides the replay length on tv.volleyballworld.com",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "css": ["main.css"],
      "matches": ["https://tv.volleyballworld.com/*"]
    }
  ]
}
